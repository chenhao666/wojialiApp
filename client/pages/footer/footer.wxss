.nav{
  width: 100%;
  height: 98rpx;
  position: fixed;
  bottom: 0rpx;
  background: #ffffff;
  border-top: 1px solid #f2f2f2;
  display: flex;
  flex-direction:row;
  justify-content: space-between;
  padding: 0px 52rpx;
  box-sizing: border-box;
}
.nav navigator{
  width: 100rpx;
  text-align: center;
}
.nav navigator view{
  width: 100rpx;
  margin:  0 auto;
  margin-top: 14rpx;
  width: 43rpx;
  height: 43rpx;
}
.nav navigator text{
  margin-top: 10rpx;
}
.nav navigator .nav_home_img{
  background: url(data:image/png;base64,) no-repeat top left;
  background-size:100% 100%; 
}
.nav navigator .nav_Uhome_img{
  background: url(data:image/png;base64,) no-repeat top left;
  background-size:100% 100%; 
}
.nav navigator .nav_user_img{
  background: url(data:image/png;base64,) no-repeat top left;
  background-size: 100% 100%;
}
.nav navigator text{
  height: 21rpx;
  line-height: 21rpx;
  text-align: center;
  font-size: 21rpx;
}
.nav navigator.active{
  color: #d2003e;
}
.nav navigator.active .nav_home_img{
  background: url(data:image/png;base64,) no-repeat top left;
  background-size:100% 100%;
}
.nav navigator.active .nav_Uhome_img{
  background: url(data:image/png;base64,) no-repeat top left;
  background-size:100% 100%;
}
.nav navigator.active .nav_user_img{
  background: url(data:image/png;base64,) no-repeat top left;
  background-size:100% 100%;
}
